###  http://creativecommons.org/ns#creator
cc:creator rdf:type owl:ObjectProperty ;
           owl:equivalentProperty :licenseAuthor .


###  http://purl.org/dc/terms/creator
dcterms:creator rdf:type owl:ObjectProperty .


###  http://purl.org/dc/terms/format
dcterms:format rdf:type owl:ObjectProperty .


###  http://purl.org/dc/terms/license
dcterms:license rdf:type owl:ObjectProperty .


###  http://spdx.org/rdf/terms#licenseException
spdx:licenseException rdf:type owl:ObjectProperty .


###  http://spdx.org/rdf/terms#member
spdx:member rdf:type owl:ObjectProperty .


###  http://www.fgw.net/core#approved
:approved rdf:type owl:ObjectProperty ;
          owl:inverseOf :approves ;
          rdfs:domain :LicenseDocument ;
          rdfs:range :LicenseValidator .


###  http://www.fgw.net/core#approves
:approves rdf:type owl:ObjectProperty ;
          rdfs:domain :LicenseValidator ;
          rdfs:range :LicenseDocument .


###  http://www.fgw.net/core#author
:author rdf:type owl:ObjectProperty ;
        rdfs:subPropertyOf dcterms:creator ,
                           foaf:maker ;
        rdfs:domain [ rdf:type owl:Class ;
                      owl:unionOf ( :LicenseDocument
                                    :LicenseException
                                    :Work
                                  )
                    ] ;
        rdfs:range :Agent .


###  http://www.fgw.net/core#contains
:contains rdf:type owl:ObjectProperty ;
          rdfs:domain [ rdf:type owl:Class ;
                        owl:unionOf ( :ConjunctiveLicenseSet
                                      :DisjunctiveLicenseSet
                                    )
                      ] ;
          rdfs:range :License ;
          rdfs:seeAlso spdx:member ,
                       rdfs:ContainerMembershipProperty ,
                       rdfs:member .


###  http://www.fgw.net/core#derivedFrom
:derivedFrom rdf:type owl:ObjectProperty ;
             rdfs:domain :Work ;
             rdfs:range :Work ;
             rdfs:seeAlso dcterms:source .


###  http://www.fgw.net/core#exception
:exception rdf:type owl:ObjectProperty ;
           rdfs:domain :LicenseWithExceptions ;
           rdfs:range :LicenseException ;
           owl:propertyChainAxiom ( spdx:licenseException
                                  ) .


###  http://www.fgw.net/core#format
:format rdf:type owl:ObjectProperty ;
        rdfs:subPropertyOf dcterms:format ;
        rdfs:domain :Work ;
        rdfs:range :FileFormat .


###  http://www.fgw.net/core#foundIn
:foundIn rdf:type owl:ObjectProperty ;
         rdfs:domain [ rdf:type owl:Class ;
                       owl:unionOf ( :Collection
                                     :Work
                                   )
                     ] ;
         rdfs:range [ rdf:type owl:Class ;
                      owl:unionOf ( :Collection
                                    :Platform
                                  )
                    ] .


###  http://www.fgw.net/core#hasCreated
:hasCreated rdf:type owl:ObjectProperty ;
            rdfs:subPropertyOf owl:topObjectProperty .


###  http://www.fgw.net/core#hasCreatedLicense
:hasCreatedLicense rdf:type owl:ObjectProperty ;
                   rdfs:subPropertyOf :hasCreated ;
                   owl:inverseOf :licenseAuthor ;
                   rdfs:domain :LicenseAuthor ;
                   rdfs:range [ rdf:type owl:Class ;
                                owl:unionOf ( :LicenseDocument
                                              :LicenseException
                                            )
                              ] .


###  http://www.fgw.net/core#license
:license rdf:type owl:ObjectProperty ;
         rdfs:subPropertyOf dcterms:license ;
         rdfs:domain :Work ;
         rdfs:range :License .


###  http://www.fgw.net/core#licenseAuthor
:licenseAuthor rdf:type owl:ObjectProperty ;
               rdfs:subPropertyOf :author ;
               rdfs:domain [ rdf:type owl:Class ;
                             owl:unionOf ( :LicenseDocument
                                           :LicenseException
                                         )
                           ] ;
               rdfs:range :LicenseAuthor .


###  http://www.fgw.net/core#licenseDocument
:licenseDocument rdf:type owl:ObjectProperty ;
                 rdfs:domain :LicenseWithExceptions ;
                 rdfs:range :LicenseException .


###  http://www.fgw.net/core#licenseLevel
:licenseLevel rdf:type owl:ObjectProperty ;
              rdfs:domain :License ;
              rdfs:range :LicenseLevel .


###  http://www.fgw.net/core#managedBy
:managedBy rdf:type owl:ObjectProperty ;
           rdfs:domain :Platform ;
           rdfs:range :Agent .


###  http://www.fgw.net/core#profileIn
:profileIn rdf:type owl:ObjectProperty ;
           rdfs:domain :PlatformUser ;
           rdfs:range :Platform .


###  http://www.fgw.net/core#type
:type rdf:type owl:ObjectProperty ;
      rdfs:domain :Work ;
      rdfs:range :WorkType .


###  http://www.fgw.net/core#uses
:uses rdf:type owl:ObjectProperty ;
      rdfs:domain :PlatformUser ;
      rdfs:range :Platform .


###  http://www.w3.org/2000/01/rdf-schema#ContainerMembershipProperty
rdfs:ContainerMembershipProperty rdf:type owl:ObjectProperty .


###  http://www.w3.org/2000/01/rdf-schema#member
rdfs:member rdf:type owl:ObjectProperty .


###  http://xmlns.com/foaf/0.1/maker
foaf:maker rdf:type owl:ObjectProperty .


