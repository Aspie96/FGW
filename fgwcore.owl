@prefix : <http://www.fgw.net/core#> .
@prefix cc: <http://creativecommons.org/ns#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix spdx: <http://spdx.org/rdf/terms#> .
@prefix vann: <http://purl.org/vocab/vann/> .
@prefix schema: <https://schema.org/> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@base <http://www.fgw.net/core> .

<http://www.fgw.net/core> rdf:type owl:Ontology ;
                           dc:creator "Valentino Giudice"^^xsd:string ;
                           dc:date "2021-07-14T00:00:00-00:00"^^xsd:dateTimeStamp ;
                           dc:description """Core of the FGW ontology.
This is the T-box for the ontology and contains all classes, as well as individuals needed to specify enumerated classes."""@en ;
                           dc:title "Core of the FGW Ontology"@en ;
                           vann:preferredNamespacePrefix "fgwcore"^^xsd:token ;
                           vann:preferredNamespaceUri "http://www.fgw.net/core#"^^xsd:anyURI ;
                           rdfs:comment """Core of the FGW ontology.
This is the T-box for the ontology and contains all classes, as well as individuals needed to specify enumerated classes."""@en ;
                           rdfs:label "Core of the FGW Ontology"@en .

#################################################################
#    Annotation properties
#################################################################

###  http://purl.org/dc/elements/1.1/creator
dc:creator rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/elements/1.1/date
dc:date rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/elements/1.1/description
dc:description rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/elements/1.1/title
dc:title rdf:type owl:AnnotationProperty .


###  http://purl.org/vocab/vann/preferredNamespacePrefix
vann:preferredNamespacePrefix rdf:type owl:AnnotationProperty .


###  http://purl.org/vocab/vann/preferredNamespaceUri
vann:preferredNamespaceUri rdf:type owl:AnnotationProperty .


#################################################################
#    Object Properties
#################################################################

###  http://creativecommons.org/ns#creator
cc:creator rdf:type owl:ObjectProperty ;
           owl:equivalentProperty :licenseAuthor .


###  http://purl.org/dc/terms/creator
dcterms:creator rdf:type owl:ObjectProperty .


###  http://purl.org/dc/terms/format
dcterms:format rdf:type owl:ObjectProperty .


###  http://purl.org/dc/terms/license
dcterms:license rdf:type owl:ObjectProperty .


###  http://spdx.org/rdf/terms#licenseException
spdx:licenseException rdf:type owl:ObjectProperty .


###  http://spdx.org/rdf/terms#member
spdx:member rdf:type owl:ObjectProperty .


###  http://www.fgw.net/core#approved
:approved rdf:type owl:ObjectProperty ;
          owl:inverseOf :approves ;
          rdfs:domain :LicenseDocument ;
          rdfs:range :LicenseValidator ;
          rdfs:comment "Specifies a license validator by which the license document was approved."@en ;
          rdfs:label "approved"@en .


###  http://www.fgw.net/core#approves
:approves rdf:type owl:ObjectProperty ;
          rdfs:domain :LicenseValidator ;
          rdfs:range :LicenseDocument ;
          rdfs:comment "Specifies a license document which the license validator approves."@en ;
          rdfs:label "approves"@en .


###  http://www.fgw.net/core#author
:author rdf:type owl:ObjectProperty ;
        rdfs:subPropertyOf dcterms:creator ,
                           foaf:maker ;
        rdf:type owl:FunctionalProperty ;
        rdfs:domain [ rdf:type owl:Class ;
                      owl:unionOf ( :Collection
                                    :LicenseDocument
                                    :LicenseException
                                    :Work
                                  )
                    ] ;
        rdfs:range :Agent ;
        rdfs:comment "Specifies the author."@en ;
        rdfs:label "author"@en .


###  http://www.fgw.net/core#collects
:collects rdf:type owl:ObjectProperty ;
          owl:inverseOf :foundIn ;
          rdf:type owl:TransitiveProperty ;
          rdfs:domain [ rdf:type owl:Class ;
                        owl:unionOf ( :Collection
                                      :Platform
                                    )
                      ] ;
          rdfs:range [ rdf:type owl:Class ;
                       owl:unionOf ( :Collection
                                     :Work
                                   )
                     ] ;
          rdfs:comment "Specifies works or other collections in the collection."@en ;
          rdfs:label "collects"@en .


###  http://www.fgw.net/core#collectsWork
:collectsWork rdf:type owl:ObjectProperty ;
              rdfs:subPropertyOf :collects ;
              rdfs:domain [ rdf:type owl:Class ;
                            owl:unionOf ( :Collection
                                          :Platform
                                        )
                          ] ;
              rdfs:range :Work ;
              rdfs:comment "Specifies a work part of the collection or platform"@en ;
              rdfs:label "collects work"@en .


###  http://www.fgw.net/core#derivedFrom
:derivedFrom rdf:type owl:ObjectProperty ;
             rdfs:domain :Work ;
             rdfs:range :Work ;
             rdfs:comment "Specifies a work the work is a derivative of."@en ;
             rdfs:label "derived from"@en ;
             rdfs:seeAlso dcterms:source .


###  http://www.fgw.net/core#exception
:exception rdf:type owl:ObjectProperty ;
           rdfs:domain :LicenseWithExceptions ;
           rdfs:range :LicenseException ;
           owl:propertyChainAxiom ( spdx:licenseException
                                  ) ;
           rdfs:comment "Specifies an exception applied to the license."@en ;
           rdfs:label "exception"@en .


###  http://www.fgw.net/core#exceptionAppliedTo
:exceptionAppliedTo rdf:type owl:ObjectProperty ;
                    rdfs:domain :LicenseWithExceptions ;
                    rdfs:range :LicenseException ;
                    rdfs:comment "exception applied to"@en ;
                    rdfs:label "Specifies to which license the exceptions are applied."@en .


###  http://www.fgw.net/core#format
:format rdf:type owl:ObjectProperty ;
        rdfs:subPropertyOf dcterms:format ;
        rdfs:domain :Work ;
        rdfs:range :FileFormat ;
        rdfs:comment "Specifies a format in which the work is available."@en ;
        rdfs:label "format"@en .


###  http://www.fgw.net/core#foundIn
:foundIn rdf:type owl:ObjectProperty ,
                  owl:TransitiveProperty ;
         rdfs:domain [ rdf:type owl:Class ;
                       owl:unionOf ( :Collection
                                     :Work
                                   )
                     ] ;
         rdfs:range [ rdf:type owl:Class ;
                      owl:unionOf ( :Collection
                                    :Platform
                                  )
                    ] ;
         rdfs:comment "Specifies a collection or platform containing the collection or work."@en ;
         rdfs:label "found in"@en .


###  http://www.fgw.net/core#hasCreated
:hasCreated rdf:type owl:ObjectProperty ;
            rdfs:subPropertyOf owl:topObjectProperty ;
            rdf:type owl:InverseFunctionalProperty ;
            rdfs:domain :Agent ;
            rdfs:range [ rdf:type owl:Class ;
                         owl:unionOf ( :Collection
                                       :LicenseDocument
                                       :LicenseException
                                       :Work
                                     )
                       ] ;
            rdfs:comment "Specifies something created by the author."@en ;
            rdfs:label "has created"@en .


###  http://www.fgw.net/core#hasCreatedLicense
:hasCreatedLicense rdf:type owl:ObjectProperty ;
                   rdfs:subPropertyOf :hasCreated ;
                   owl:inverseOf :licenseAuthor ;
                   rdf:type owl:InverseFunctionalProperty ;
                   rdfs:domain :LicenseAuthor ;
                   rdfs:range [ rdf:type owl:Class ;
                                owl:unionOf ( :LicenseDocument
                                              :LicenseException
                                            )
                              ] ;
                   rdfs:comment "Specifies a license document or license exception created by the license author."@en ;
                   rdfs:label "has created license"@en .


###  http://www.fgw.net/core#license
:license rdf:type owl:ObjectProperty ;
         rdfs:subPropertyOf dcterms:license ;
         rdf:type owl:FunctionalProperty ;
         rdfs:domain :Work ;
         rdfs:range :License .


###  http://www.fgw.net/core#licenseAuthor
:licenseAuthor rdf:type owl:ObjectProperty ;
               rdfs:subPropertyOf :author ;
               rdf:type owl:FunctionalProperty ;
               rdfs:domain [ rdf:type owl:Class ;
                             owl:unionOf ( :LicenseDocument
                                           :LicenseException
                                         )
                           ] ;
               rdfs:range :LicenseAuthor ;
               rdfs:comment "Specifies the author of the license or license exception."@en ;
               rdfs:label "license author"@en .


###  http://www.fgw.net/core#licenseLevel
:licenseLevel rdf:type owl:ObjectProperty ,
                       owl:FunctionalProperty ;
              rdfs:domain :License ;
              rdfs:range :LicenseLevel ;
              rdfs:comment "Specifies how restrictive a license is."@en ;
              rdfs:label "license level"@en .


###  http://www.fgw.net/core#memberLicense
:memberLicense rdf:type owl:ObjectProperty ;
               rdfs:domain [ rdf:type owl:Class ;
                             owl:unionOf ( :ConjunctiveLicenseSet
                                           :DisjunctiveLicenseSet
                                         )
                           ] ;
               rdfs:range :License ;
               rdfs:comment "Specifies a license in the set."@en ;
               rdfs:label "contains"@en ;
               rdfs:seeAlso spdx:member ,
                            rdfs:ContainerMembershipProperty ,
                            rdfs:member .


###  http://www.fgw.net/core#type
:type rdf:type owl:ObjectProperty ,
               owl:FunctionalProperty ;
      rdfs:domain :Work ;
      rdfs:range :WorkType ;
      rdfs:comment "Specifies the type of a work."@en ;
      rdfs:label "type"@en .


###  http://www.fgw.net/core#uses
:uses rdf:type owl:ObjectProperty ,
               owl:FunctionalProperty ;
      rdfs:domain :PlatformUser ;
      rdfs:range :Platform ;
      rdfs:comment "Specifies the platform used by the user."@en ;
      rdfs:label "uses"@en .


###  http://www.fgw.net/core#workLicenseLevel
:workLicenseLevel rdf:type owl:ObjectProperty ;
                  rdfs:domain :Work ;
                  rdfs:range :LicenseLevel ;
                  owl:propertyChainAxiom ( :license
                                           :licenseLevel
                                         ) ;
                  rdfs:comment "Specifies how restrictive the licensing of a work is"@en ;
                  rdfs:label "work license level"@en .


###  http://www.w3.org/2000/01/rdf-schema#ContainerMembershipProperty
rdfs:ContainerMembershipProperty rdf:type owl:ObjectProperty .


###  http://www.w3.org/2000/01/rdf-schema#member
rdfs:member rdf:type owl:ObjectProperty .


###  http://xmlns.com/foaf/0.1/maker
foaf:maker rdf:type owl:ObjectProperty .


#################################################################
#    Data properties
#################################################################

###  http://purl.org/dc/terms/language
dcterms:language rdf:type owl:DatatypeProperty ;
                 owl:equivalentProperty :language ;
                 rdfs:subPropertyOf owl:topDataProperty .


###  http://purl.org/dc/terms/source
dcterms:source rdf:type owl:DatatypeProperty .


###  http://www.fgw.net/core#extension
:extension rdf:type owl:DatatypeProperty ;
           rdfs:domain :FileFormat ;
           rdfs:range xsd:token ;
           rdfs:comment "Specifies a typical extension for the format."@en ;
           rdfs:label "extension"@en .


###  http://www.fgw.net/core#language
:language rdf:type owl:DatatypeProperty ;
          rdfs:range xsd:language ;
          rdfs:comment "Specifies a language."@en ;
          rdfs:label "language"@en .


###  http://www.fgw.net/core#licenseList
:licenseList rdf:type owl:DatatypeProperty ;
             rdfs:subPropertyOf :location ;
             rdfs:domain :LicenseValidator ;
             rdfs:range xsd:anyURI ;
             rdfs:comment "Specifies a URL to a list of the approved licenses."@en .


###  http://www.fgw.net/core#licenseSpdx
:licenseSpdx rdf:type owl:DatatypeProperty ,
                      owl:FunctionalProperty ;
             rdfs:domain [ rdf:type owl:Class ;
                           owl:unionOf ( :LicenseDocument
                                         :LicenseException
                                       )
                         ] ;
             rdfs:range xsd:token ;
             rdfs:comment "Specifies the SPDX identifier of the license document."@en ;
             rdfs:label "license SPDX"@en .


###  http://www.fgw.net/core#location
:location rdf:type owl:DatatypeProperty ;
          rdfs:domain [ rdf:type owl:Class ;
                        owl:unionOf ( :Collection
                                      :Platform
                                      :PlatformUser
                                      :Work
                                    )
                      ] ;
          rdfs:range xsd:anyURI ;
          rdfs:comment "Specifies a URL."@en ;
          rdfs:label "location"@en .


###  http://www.fgw.net/core#mime
:mime rdf:type owl:DatatypeProperty ;
      rdfs:domain :FileFormat ;
      rdfs:range xsd:token ;
      rdfs:comment "Specifies the MIME type of the format."@en ;
      rdfs:label "MIME"@en .


###  http://www.fgw.net/core#notice
:notice rdf:type owl:DatatypeProperty ,
                 owl:FunctionalProperty ;
        rdfs:domain :Work ;
        rdfs:range xsd:string ;
        rdfs:comment "Specifies a notice to be read in addition to the license."@en ;
        rdfs:label "notice"@en .


###  http://www.fgw.net/core#tag
:tag rdf:type owl:DatatypeProperty ;
     rdfs:domain :Work ;
     rdfs:range xsd:string ;
     rdfs:comment "Specifies a tag for the work."@en ;
     rdfs:label "tag"@en .


#################################################################
#    Classes
#################################################################

###  http://purl.org/dc/terms/Agent
dcterms:Agent rdf:type owl:Class ;
              owl:equivalentClass :Agent .


###  http://purl.org/dc/terms/FileFormat
dcterms:FileFormat rdf:type owl:Class ;
                   owl:equivalentClass :FileFormat .


###  http://purl.org/dc/terms/LicenseDocument
dcterms:LicenseDocument rdf:type owl:Class ;
                        owl:equivalentClass :License .


###  http://schema.org/Collection
<http://schema.org/Collection> rdf:type owl:Class .


###  http://schema.org/CreativeWork
<http://schema.org/CreativeWork> rdf:type owl:Class .


###  http://spdx.org/rdf/terms#AnyLicenseInfo
spdx:AnyLicenseInfo rdf:type owl:Class ;
                    owl:equivalentClass :License .


###  http://spdx.org/rdf/terms#ConjunctiveLicenseSet
spdx:ConjunctiveLicenseSet rdf:type owl:Class ;
                           owl:equivalentClass :ConjunctiveLicenseSet .


###  http://spdx.org/rdf/terms#DisjunctiveLicenseSet
spdx:DisjunctiveLicenseSet rdf:type owl:Class ;
                           owl:equivalentClass :DisjunctiveLicenseSet .


###  http://spdx.org/rdf/terms#LicenseException
spdx:LicenseException rdf:type owl:Class ;
                      owl:equivalentClass :LicenseException .


###  http://spdx.org/rdf/terms#SimpleLicensingInfo
spdx:SimpleLicensingInfo rdf:type owl:Class ;
                         owl:equivalentClass :LicenseDocument .


###  http://spdx.org/rdf/terms#WithExceptionOperator
spdx:WithExceptionOperator rdf:type owl:Class ;
                           rdfs:subClassOf :LicenseWithExceptions .


###  http://www.fgw.net/core#Agent
:Agent rdf:type owl:Class ;
       owl:equivalentClass foaf:Agent ;
       owl:disjointWith :Collection ,
                        :FileFormat ,
                        :License ,
                        :LicenseException ,
                        :LicenseLevel ,
                        :Platform ,
                        :Work ,
                        :WorkType ;
       rdfs:comment "Represents an agent which is responsable for actions."@en ;
       rdfs:label "Agent"@en .


###  http://www.fgw.net/core#Collection
:Collection rdf:type owl:Class ;
            rdfs:subClassOf <http://schema.org/Collection> ,
                            [ rdf:type owl:Restriction ;
                              owl:onProperty :author ;
                              owl:someValuesFrom :PlatformUser
                            ] ,
                            [ rdf:type owl:Restriction ;
                              owl:onProperty :foundIn ;
                              owl:someValuesFrom :Platform
                            ] ;
            owl:disjointWith :FileFormat ,
                             :License ,
                             :LicenseException ,
                             :LicenseLevel ,
                             :Work ,
                             :WorkType ;
            rdfs:comment "Represents a collection of works or other collections of a given platform by a user of that platform."@en ;
            rdfs:label "Collection"@en .


###  http://www.fgw.net/core#ConjunctiveLicenseSet
:ConjunctiveLicenseSet rdf:type owl:Class ;
                       rdfs:subClassOf :License ,
                                       rdfs:Container ,
                                       [ rdf:type owl:Class ;
                                         owl:unionOf ( :CopyleftLicense
                                                       [ rdf:type owl:Restriction ;
                                                         owl:onProperty :memberLicense ;
                                                         owl:allValuesFrom :PermissiveLicense
                                                       ]
                                                     )
                                       ] ,
                                       [ rdf:type owl:Class ;
                                         owl:unionOf ( :StrongCopyleftLicense
                                                       [ rdf:type owl:Class ;
                                                         owl:complementOf [ rdf:type owl:Restriction ;
                                                                            owl:onProperty :memberLicense ;
                                                                            owl:someValuesFrom :StrongCopyleftLicense
                                                                          ]
                                                       ]
                                                     )
                                       ] ,
                                       [ rdf:type owl:Class ;
                                         owl:complementOf [ owl:intersectionOf ( :StrongCopyleftLicense
                                                                                 [ rdf:type owl:Class ;
                                                                                   owl:complementOf [ rdf:type owl:Restriction ;
                                                                                                      owl:onProperty :memberLicense ;
                                                                                                      owl:someValuesFrom :StrongCopyleftLicense
                                                                                                    ]
                                                                                 ]
                                                                               ) ;
                                                            rdf:type owl:Class
                                                          ]
                                       ] ,
                                       [ rdf:type owl:Class ;
                                         owl:complementOf [ owl:intersectionOf ( [ rdf:type owl:Class ;
                                                                                   owl:complementOf :PermissiveLicense
                                                                                 ]
                                                                                 [ rdf:type owl:Restriction ;
                                                                                   owl:onProperty :memberLicense ;
                                                                                   owl:allValuesFrom :PermissiveLicense
                                                                                 ]
                                                                               ) ;
                                                            rdf:type owl:Class
                                                          ]
                                       ] ,
                                       [ rdf:type owl:Restriction ;
                                         owl:onProperty :memberLicense ;
                                         owl:minQualifiedCardinality "2"^^xsd:nonNegativeInteger ;
                                         owl:onClass :License
                                       ] ;
                       owl:hasKey ( :memberLicense
                                  ) ;
                       rdfs:comment """Represents a conjunction of multiple licenses.
This means the user has to comply with the terms of all licenses in the set."""@en ;
                       rdfs:label "Conjunctive license set"@en .


###  http://www.fgw.net/core#CopyleftLicense
:CopyleftLicense rdf:type owl:Class ;
                 rdfs:subClassOf :License ;
                 owl:disjointUnionOf ( :StrongCopyleftLicense
                                       :WeakCopyleftLicense
                                     ) ;
                 rdfs:comment "A license requiring that derived works are released under a compatible license."@en ;
                 rdfs:label "Copyleft license"@en .


###  http://www.fgw.net/core#DisjunctiveLicenseSet
:DisjunctiveLicenseSet rdf:type owl:Class ;
                       rdfs:subClassOf :License ,
                                       rdfs:Container ,
                                       [ rdf:type owl:Class ;
                                         owl:unionOf ( :PermissiveLicense
                                                       [ rdf:type owl:Restriction ;
                                                         owl:onProperty :memberLicense ;
                                                         owl:allValuesFrom :CopyleftLicense
                                                       ]
                                                     )
                                       ] ,
                                       [ rdf:type owl:Class ;
                                         owl:unionOf ( [ rdf:type owl:Class ;
                                                         owl:complementOf :StrongCopyleftLicense
                                                       ]
                                                       [ rdf:type owl:Restriction ;
                                                         owl:onProperty :memberLicense ;
                                                         owl:allValuesFrom :StrongCopyleftLicense
                                                       ]
                                                     )
                                       ] ,
                                       [ rdf:type owl:Class ;
                                         owl:complementOf [ owl:intersectionOf ( :PermissiveLicense
                                                                                 [ rdf:type owl:Restriction ;
                                                                                   owl:onProperty :memberLicense ;
                                                                                   owl:allValuesFrom :CopyleftLicense
                                                                                 ]
                                                                               ) ;
                                                            rdf:type owl:Class
                                                          ]
                                       ] ,
                                       [ rdf:type owl:Class ;
                                         owl:complementOf [ owl:intersectionOf ( [ rdf:type owl:Class ;
                                                                                   owl:complementOf :StrongCopyleftLicense
                                                                                 ]
                                                                                 [ rdf:type owl:Restriction ;
                                                                                   owl:onProperty :memberLicense ;
                                                                                   owl:allValuesFrom :StrongCopyleftLicense
                                                                                 ]
                                                                               ) ;
                                                            rdf:type owl:Class
                                                          ]
                                       ] ,
                                       [ rdf:type owl:Restriction ;
                                         owl:onProperty :memberLicense ;
                                         owl:minQualifiedCardinality "2"^^xsd:nonNegativeInteger ;
                                         owl:onClass :License
                                       ] ;
                       owl:hasKey ( :memberLicense
                                  ) ;
                       rdfs:comment "Represents a set of licenses among which the user may chose. The user only has to comply with the terms of at least one license at their choice in the set and receives all permissions from all licenses they comply with."@en ;
                       rdfs:label "Disjunctive license set"@en .


###  http://www.fgw.net/core#FileFormat
:FileFormat rdf:type owl:Class ;
            rdfs:subClassOf [ rdf:type owl:Restriction ;
                              owl:onProperty :extension ;
                              owl:someValuesFrom xsd:token
                            ] ;
            owl:disjointWith :License ,
                             :LicenseException ,
                             :LicenseLevel ,
                             :Platform ,
                             :Work ,
                             :WorkType ;
            rdfs:comment "Represents a type of file and its encoding."@en ;
            rdfs:label "File format"@en .


###  http://www.fgw.net/core#LargeCollection
:LargeCollection rdf:type owl:Class ;
                 owl:equivalentClass [ owl:intersectionOf ( :Collection
                                                            [ rdf:type owl:Restriction ;
                                                              owl:onProperty :collectsWork ;
                                                              owl:minQualifiedCardinality "10"^^xsd:nonNegativeInteger ;
                                                              owl:onClass :Work
                                                            ]
                                                          ) ;
                                       rdf:type owl:Class
                                     ] ;
                 rdfs:subClassOf :Collection ;
                 rdfs:comment "Represents a large collection with at least 10 elements."@en ;
                 rdfs:label "large collection"@en .


###  http://www.fgw.net/core#License
:License rdf:type owl:Class ;
         owl:equivalentClass [ rdf:type owl:Restriction ;
                               owl:onProperty :licenseLevel ;
                               owl:someValuesFrom :LicenseLevel
                             ] ;
         rdfs:subClassOf foaf:Document ;
         owl:disjointWith :LicenseException ,
                          :LicenseLevel ,
                          :Platform ,
                          :Work ,
                          :WorkType ;
         owl:disjointUnionOf ( :ConjunctiveLicenseSet
                               :LicenseDocument
                               :LicenseWithExceptions
                             ) ,
                             ( :CopyleftLicense
                               :PermissiveLicense
                             ) ;
         rdfs:comment """Describes a licensing status a work may have.
A license is a potentially complex entity. It may correspond to a simple license document, a license document with one or more exceptions, or a conjunction or disjunction of multiple licenses."""@en ;
         rdfs:label "License"@en .


###  http://www.fgw.net/core#LicenseAuthor
:LicenseAuthor rdf:type owl:Class ;
               owl:equivalentClass [ rdf:type owl:Restriction ;
                                     owl:onProperty :hasCreatedLicense ;
                                     owl:someValuesFrom [ rdf:type owl:Class ;
                                                          owl:unionOf ( :License
                                                                        :LicenseException
                                                                      )
                                                        ]
                                   ] ;
               rdfs:subClassOf :Agent ;
               rdfs:comment "Represents an agent which has written one or more licenses or license exceptions."@en ;
               rdfs:label "License author"@en .


###  http://www.fgw.net/core#LicenseDocument
:LicenseDocument rdf:type owl:Class ;
                 rdfs:subClassOf :License ;
                 rdfs:comment "Represents a licensing document which, subject to certain conditions, allows certain uses of a work."@en ;
                 rdfs:label "License document"@en .


###  http://www.fgw.net/core#LicenseException
:LicenseException rdf:type owl:Class ;
                  owl:disjointWith :LicenseLevel ,
                                   :Platform ,
                                   :Work ,
                                   :WorkType ;
                  rdfs:comment """Represents an exception which can be applied to a license document, reducing its restriction.
An exception can always be ignored by the user of any work using that license, since it merely provides additional permissions."""@en ;
                  rdfs:label "License exception"@en .


###  http://www.fgw.net/core#LicenseLevel
:LicenseLevel rdf:type owl:Class ;
              owl:equivalentClass [ rdf:type owl:Class ;
                                    owl:oneOf ( :permissive
                                                :strongCopyleft
                                                :weakCopyleft
                                              )
                                  ] ;
              owl:disjointWith :Platform ,
                               :Work ,
                               :WorkType ;
              rdfs:comment "Represents how restrictive a license is."@en ;
              rdfs:label "License level"@en .


###  http://www.fgw.net/core#LicenseValidator
:LicenseValidator rdf:type owl:Class ;
                  owl:equivalentClass [ rdf:type owl:Restriction ;
                                        owl:onProperty :approves ;
                                        owl:someValuesFrom :LicenseDocument
                                      ] ;
                  rdfs:subClassOf :Agent ,
                                  [ rdf:type owl:Restriction ;
                                    owl:onProperty :licenseList ;
                                    owl:someValuesFrom xsd:anyURI
                                  ] ;
                  rdfs:comment """Represents an agent which provides a list of free license documents.
When a license document is in the list it means that the agent certifies that the license qualifies as free."""@en ;
                  rdfs:label "License validator"@en .


###  http://www.fgw.net/core#LicenseWithExceptions
:LicenseWithExceptions rdf:type owl:Class ;
                       rdfs:subClassOf :License ,
                                       [ rdf:type owl:Class ;
                                         owl:unionOf ( :PermissiveLicense
                                                       [ rdf:type owl:Restriction ;
                                                         owl:onProperty :exceptionAppliedTo ;
                                                         owl:someValuesFrom :CopyleftLicense
                                                       ]
                                                     )
                                       ] ,
                                       [ rdf:type owl:Class ;
                                         owl:complementOf [ owl:intersectionOf ( :StrongCopyleftLicense
                                                                                 [ rdf:type owl:Class ;
                                                                                   owl:complementOf [ rdf:type owl:Restriction ;
                                                                                                      owl:onProperty :exceptionAppliedTo ;
                                                                                                      owl:someValuesFrom :StrongCopyleftLicense
                                                                                                    ]
                                                                                 ]
                                                                               ) ;
                                                            rdf:type owl:Class
                                                          ]
                                       ] ,
                                       [ rdf:type owl:Restriction ;
                                         owl:onProperty :exception ;
                                         owl:someValuesFrom :LicenseException
                                       ] ,
                                       [ rdf:type owl:Restriction ;
                                         owl:onProperty :exceptionAppliedTo ;
                                         owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                         owl:onClass :LicenseDocument
                                       ] ;
                       owl:hasKey ( :exception
                                  ) ,
                                  ( :exceptionAppliedTo
                                  ) ;
                       rdfs:comment """Represents a license documents to which some exceptions have been applied.
Any use in compliance with the license ignoring the exceptions is still allowed, as is ignoring any specific license exception or group of license exceptions.
However, license exceptions may allow some additional uses or remove some restrictions."""@en ;
                       rdfs:label "License with exceptions"@en .


###  http://www.fgw.net/core#PermissiveLicense
:PermissiveLicense rdf:type owl:Class ;
                   owl:equivalentClass [ owl:intersectionOf ( :License
                                                              [ rdf:type owl:Restriction ;
                                                                owl:onProperty :licenseLevel ;
                                                                owl:hasValue :permissive
                                                              ]
                                                            ) ;
                                         rdf:type owl:Class
                                       ] ;
                   rdfs:subClassOf :License ;
                   rdfs:comment "A free license which doesn't have copyleft."@en ;
                   rdfs:label "Permissive license"@en .


###  http://www.fgw.net/core#Platform
:Platform rdf:type owl:Class ;
          rdfs:subClassOf [ rdf:type owl:Restriction ;
                            owl:onProperty :location ;
                            owl:someValuesFrom xsd:anyURI
                          ] ;
          owl:disjointWith :Work ,
                           :WorkType ;
          rdfs:comment "Represents a website in which works are published."@en ;
          rdfs:label "Platform"@en .


###  http://www.fgw.net/core#PlatformUser
:PlatformUser rdf:type owl:Class ;
              rdfs:subClassOf :Agent ,
                              [ rdf:type owl:Restriction ;
                                owl:onProperty :uses ;
                                owl:someValuesFrom :Platform
                              ] ;
              rdfs:comment "Represents a user of some platofrm, who may have created works or collections in that platform."@en ;
              rdfs:label "Platform user"@en ;
              rdfs:seeAlso foaf:OnlineAccount .


###  http://www.fgw.net/core#StrongCopyleftLicense
:StrongCopyleftLicense rdf:type owl:Class ;
                       owl:equivalentClass [ owl:intersectionOf ( :License
                                                                  [ rdf:type owl:Restriction ;
                                                                    owl:onProperty :licenseLevel ;
                                                                    owl:hasValue :strongCopyleft
                                                                  ]
                                                                ) ;
                                             rdf:type owl:Class
                                           ] ;
                       rdfs:subClassOf :CopyleftLicense ;
                       rdfs:label "A copyleft license which requires that when works are combined with the licensed work they are released under a compatible license."@en ,
                                  "Strong copyleft license"@en .


###  http://www.fgw.net/core#WeakCopyleftLicense
:WeakCopyleftLicense rdf:type owl:Class ;
                     owl:equivalentClass [ owl:intersectionOf ( :License
                                                                [ rdf:type owl:Restriction ;
                                                                  owl:onProperty :licenseLevel ;
                                                                  owl:hasValue :weakCopyleft
                                                                ]
                                                              ) ;
                                           rdf:type owl:Class
                                         ] ;
                     rdfs:subClassOf :CopyleftLicense ;
                     rdfs:comment "A copyleft license which doesn't require that works which are combined with the licensed work into a single resulting works are released under a compatible license."@en ;
                     rdfs:label "Weak copyleft license"@en .


###  http://www.fgw.net/core#Work
:Work rdf:type owl:Class ;
      rdfs:subClassOf <http://schema.org/CreativeWork> ,
                      [ rdf:type owl:Restriction ;
                        owl:onProperty :author ;
                        owl:someValuesFrom :PlatformUser
                      ] ,
                      [ rdf:type owl:Restriction ;
                        owl:onProperty :foundIn ;
                        owl:someValuesFrom :Platform
                      ] ,
                      [ rdf:type owl:Restriction ;
                        owl:onProperty :license ;
                        owl:someValuesFrom :License
                      ] ;
      owl:disjointWith :WorkType ;
      rdfs:comment "Represents a creative work with a license for use present on a platform."@en ;
      rdfs:label "Work"@en .


